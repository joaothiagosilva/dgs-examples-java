{
  "request": {
    "method": "POST",
    "url": "/reviews",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "equalToJson": "[1,2,3,4,5]",
        "ignoreArrayOrder": true,
        "ignoreExtraElements": true
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "reviews/default.json",
    "headers": {
      "Content-Type": "application/json;charset=UTF-8"
    }
  }
}